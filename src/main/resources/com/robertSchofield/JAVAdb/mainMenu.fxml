<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.robertSchofield.JAVAdb.controller.mainMenuController">

  <children>
    <VBox alignment="CENTER" layoutX="100.0" layoutY="100.0" prefHeight="200.0" prefWidth="400.0" spacing="20.0">
      <children>
        <Label alignment="CENTER" styleClass="title" text="Main Menu" textAlignment="CENTER">
               <font>
                  <Font name="Helvetica Neue" size="24.0" />
               </font></Label>
        <Button mnemonicParsing="false" onAction="#menuAppoint" text="Appointments">
               <font>
                  <Font name="Helvetica Neue" size="13.0" />
               </font></Button>
        <Button mnemonicParsing="false" onAction="#menuReport" text="Reports">
               <font>
                  <Font name="Helvetica Neue" size="13.0" />
               </font></Button>
        <Button mnemonicParsing="false" onAction="#menuCustomer" text="Customers">
               <font>
                  <Font name="Helvetica Neue" size="13.0" />
               </font></Button>
        <Button layoutX="512.0" layoutY="345.0" mnemonicParsing="false" onAction="#menuExit"
            prefWidth="55.0" text="Exit">
          <font>
            <Font name="Helvetica Neue" size="13.0"/>
          </font>
        </Button>
      </children>
    </VBox>
  </children>
</AnchorPane>
